# API Configuration
# Use local proxy (default) or direct API URL
NEXT_PUBLIC_API_URL=/api/clearml
# NEXT_PUBLIC_API_URL=https://api.clear.ml/v2.0

NEXT_PUBLIC_WEB_SERVER_URL=https://app.clear.ml

# App Configuration
NEXT_PUBLIC_APP_NAME=ClearML Web
NEXT_PUBLIC_APP_VERSION=2.2.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# ClearML API Credentials (for development only)
# Get these from your ClearML server settings or credentials.json
# Format: Basic Auth with access_key:secret_key
NEXT_PUBLIC_CLEARML_ACCESS_KEY=your_access_key_here
NEXT_PUBLIC_CLEARML_SECRET_KEY=your_secret_key_here

# Example from Angular app's base.ts (DO NOT USE IN PRODUCTION):
# NEXT_PUBLIC_CLEARML_ACCESS_KEY=EYVQ385RW7Y2QQUH88CZ7DWIQ1WUHP
# NEXT_PUBLIC_CLEARML_SECRET_KEY=XhkH6a6ds9JBnM_MrahYyYdO-wS2bqFSm8gl-V0UZXH26Ydd6Eyi28TeBEoSr6Z3Bes
