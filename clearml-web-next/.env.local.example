# API Configuration
NEXT_PUBLIC_API_URL=https://api.clear.ml/v2.0
NEXT_PUBLIC_WEB_SERVER_URL=https://app.clear.ml

# App Configuration
NEXT_PUBLIC_APP_NAME=ClearML Web
NEXT_PUBLIC_APP_VERSION=2.2.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# Authentication
NEXT_PUBLIC_AUTH_COOKIE_NAME=clearml_token

# ClearML Simple Mode Credentials (optional - can also use credentials.json)
# These are used for passwordless authentication in simple mode
# NEXT_PUBLIC_USER_KEY=your_user_key_here
# NEXT_PUBLIC_USER_SECRET=your_user_secret_here
# NEXT_PUBLIC_COMPANY_ID=your_company_id_here
